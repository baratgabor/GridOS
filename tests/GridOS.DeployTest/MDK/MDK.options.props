<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <!-- 
    Contains basic settings that should be included in code repositories
    -->
    <MDKVersion>1.2.31</MDKVersion>
    <MDKTrimTypes>
      <Enabled>yes</Enabled>
    </MDKTrimTypes>
    <MDKMinify>
      <Level>StripComments</Level>
    </MDKMinify>
    <MDKIgnore>
      <Folder>mdk</Folder>
    </MDKIgnore>
  </PropertyGroup>
</Project>